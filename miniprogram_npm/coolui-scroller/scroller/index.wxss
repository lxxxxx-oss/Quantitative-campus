:host {
  display: block;
  width  : 100%;
  height : 100%;
}

.refresh-container{
  width: 100%;
}

.wrap {
  display : block;
  height  : 100%;
  position: relative;
}

.slot-header {
  position: relative;
  z-index : 5;
}

.slot-empty {
  position        : absolute;
  width           : 100%;
  bottom          : 0;
  left            : 0;
}

.wrp {
  height: 200px;
}

.backToTop {
  background-color   : rgba(0, 0, 0, 0.6);
  background-image   : url('data:image/png;base64, ');
  background-size    : 45rpx;
  background-repeat  : no-repeat;
  background-position: center;
  width              : 80rpx;
  height             : 80rpx;
  position           : fixed;
  bottom             : 40rpx;
  right              : 30rpx;
  border-radius      : 100%;
  z-index            : 5;
  opacity            : 0.6;
}