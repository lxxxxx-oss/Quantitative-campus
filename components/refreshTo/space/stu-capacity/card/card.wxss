.title {
  margin-top: 30rpx;
  font-size: 35rpx;
  text-align: center;
}
.card {
  width: 680rpx;
  height: 450rpx;
  margin-top: 50rpx;
  border: 2rpx solid #fff;
  border-radius: 50rpx;
  overflow: hidden;
  box-shadow: 20rpx 14rpx 30rpx 5rpx rgba(0, 0, 0, .2);
}
.card image {
  position: fixed; 
  z-index: -1;
}
.moneyText {
  font-size: 60rpx;
  margin: 50rpx 0 0 40rpx;
}
.line {
  margin-top: 50rpx;
  height: 500rpx;
  width: 680rpx;
  background-color: #fff;
  border-radius: 50rpx;
  overflow: hidden;
  box-shadow: 20rpx 14rpx 30rpx 5rpx rgba(0, 0, 0, .2);
}
ec-canvas {
  width: 100%;
  height: 100%;
}
